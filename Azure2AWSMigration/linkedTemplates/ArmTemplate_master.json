{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Azure2AWSMigration"
		},
		"AWS_RDS_SQLServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AWS_RDS_SQLServer'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-db-lre-prod.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "LibroRemuneraciones"
		},
		"AWS_RDS_SQLServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "database-lre.ctggwg62oziy.sa-east-1.rds.amazonaws.com"
		},
		"AWS_RDS_SQLServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "lre_prod_db"
		},
		"AWS_RDS_SQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "migration_user"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Azure2AWSMigration_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AWS_RDS_SQLServer_password": {
						"value": "[parameters('AWS_RDS_SQLServer_password')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AWS_RDS_SQLServer_properties_typeProperties_server": {
						"value": "[parameters('AWS_RDS_SQLServer_properties_typeProperties_server')]"
					},
					"AWS_RDS_SQLServer_properties_typeProperties_database": {
						"value": "[parameters('AWS_RDS_SQLServer_properties_typeProperties_database')]"
					},
					"AWS_RDS_SQLServer_properties_typeProperties_userName": {
						"value": "[parameters('AWS_RDS_SQLServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Azure2AWSMigration_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Azure2AWSMigration_ArmTemplate_0')]"
			]
		},
		{
			"name": "Azure2AWSMigration_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Azure2AWSMigration_ArmTemplate_1')]"
			]
		}
	]
}